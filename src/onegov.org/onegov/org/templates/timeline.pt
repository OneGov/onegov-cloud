<div metal:use-macro="layout.base" i18n:domain="onegov.org">
    <tal:block metal:fill-slot="title">
        ${title}
    </tal:block>
    <tal:block metal:fill-slot="content">
        <div class="timeline"
            data-feed="${feed}"
            data-initial-data="${initial_data}"
            data-poll-interval="${poll_interval}"
        />
    </tal:block>
</div>
