<div metal:use-macro="layout.base" i18n:domain="onegov.election_day">
    <tal:block metal:fill-slot="before-content">
        <h1>
            <small>${layout.app.principal.name}</small>
            <tal:block i18n:translate="">Elections &amp; Votes</tal:block> ${year}
        </h1>

        <hr />
    </tal:block>
    <tal:block metal:fill-slot="content">
        <div class="homepage">
            <div metal:use-macro="layout.macros['archive_listing']" />
        </div>

        <div metal:use-macro="layout.macros['archive']" />

    </tal:block>
</div>