<div metal:use-macro="layout.base" i18n:domain="onegov.feriennet">
    <tal:block metal:fill-slot="title">
        ${title}
    </tal:block>
    <tal:block metal:fill-slot="content">
        <ul class="with-lead">
            <li tal:repeat="(name, export_title, lead) model.exports">
                <a class="list-title" href="${request.link(model, name)}">
                    ${export_title}
                </a>
                <div class="list-lead">${lead}</div>
            </li>
        </ul>
    </tal:block>
</div>