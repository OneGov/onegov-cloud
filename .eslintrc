{
    "parserOptions": {
        "ecmaVersion": 5,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": false,
        "es6": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "accessor-pairs": [2, {"getWithoutSet": false, "setWithoutGet": true}],
        "array-bracket-spacing": [1, "never"],
        "arrow-parens": [2, "always"],
        "arrow-spacing": [2, {"before": true, "after": true}],
        "block-scoped-var": 2,
        "block-spacing": [1, "always"],
        "brace-style": [0, "1tbs", {"allowSingleLine": false}],
        "callback-return": 0,
        "camelcase": [0, {"properties": "always"}],
        "comma-dangle": [2, "never"],
        "comma-spacing": [1, {"before": false, "after": true}],
        "comma-style": [1, "last"],
        "complexity": [1, 10],
        "computed-property-spacing": [1, "never"],
        "consistent-return": 2,
        "consistent-this": [1, "self"],
        "constructor-super": 2,
        "curly": [2, "all"],
        "default-case": 2,
        "dot-location": [2, "property"],
        "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
        "eol-last": 1,
        "eqeqeq": 2,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": [2, {"before": false, "after": true}],
        "global-require": 0,
        "guard-for-in": 2,
        "handle-callback-err": 0,
        "id-length": 0,
        "id-match": 0,
        "indent": [1, 4, {"SwitchCase": 1}],
        "init-declarations": 0,
        "jsx-quotes": [1, "prefer-double"],
        "key-spacing": [1, {"beforeColon": false, "afterColon": true, "mode": "minimum"}],
        "keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
        "linebreak-style": 0,
        "lines-around-comment": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": [1, 3],
        "max-params": 0,
        "max-statements": 0,
        "new-cap": [0, {"newIsCap": true, "capIsNew": true}],
        "new-parens": 1,
        "newline-after-var": 0,
        "no-alert": 1,
        "no-array-constructor": 1,
        "no-bitwise": 0,
        "no-caller": 2,
        "no-catch-shadow": 0,
        "no-class-assign": 2,
        "no-cond-assign": [2, "except-parens"],
        "no-console": 1,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-continue": 1,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 1,
        "no-div-regex": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 0,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 0,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [2, "functions"],
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-func-assign": 2,
        "no-implicit-coercion": [1, {"boolean": false, "number": true, "string": false}],
        "no-implied-eval": 2,
        "no-inline-comments": 0,
        "no-inner-declarations": [0, "both"],
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 1,
        "no-loop-func": 2,
        "no-mixed-requires": 0,
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-spaces": [2, {"exceptions": {"VariableDeclarator": true,  "ImportDeclaration": true, "AssignmentExpression": true, "ObjectExpression": true}}],
        "no-multi-str": 2,
        "no-multiple-empty-lines": [1, {"max": 1}],
        "no-native-reassign": 2,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 1,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 1,
        "no-new-require": 0,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 0,
        "no-path-concat": 0,
        "no-plusplus": 0,
        "no-process-env": 1,
        "no-process-exit": 0,
        "no-proto": 2,
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-regex-spaces": 2,
        "no-restricted-modules": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": [2, "except-parens"],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-shadow": [2, {"builtinGlobals": false, "hoist": "all"}],
        "no-shadow-restricted-names": 2,
        "no-spaced-func": 1,
        "no-sparse-arrays": 2,
        "no-sync": 0,
        "no-ternary": 0,
        "no-this-before-super": 2,
        "no-throw-literal": 2,
        "no-trailing-spaces": [1, {"skipBlankLines": false}],
        "no-undef": 0,
        "no-undef-init": 1,
        "no-undefined": 0,
        "no-underscore-dangle": 0,
        "no-unexpected-multiline": 2,
        "no-unneeded-ternary": [1, {"defaultAssignment": true}],
        "no-unreachable": 2,
        "no-unused-expressions": 2,
        "no-unused-vars": [1, {"vars": "local", "args": "all", "argsIgnorePattern": "^_"}],
        "no-use-before-define": 1,
        "no-useless-call": 1,
        "no-useless-concat": 2,
        "no-var": 0,
        "no-void": 2,
        "no-warning-comments": [1, {"terms": ["todo", "fixme"], "location": "start"}],
        "no-with": 2,
        "object-curly-spacing": [1, "never"],
        "object-shorthand": 0,
        "one-var": 0,
        "operator-assignment": 0,
        "operator-linebreak": [1, "after"],
        "padded-blocks": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 1,
        "prefer-reflect": 0,
        "prefer-spread": 0,
        "prefer-template": 0,
        "quote-props": [1, "as-needed", {"keywords": false, "unnecessary": false, "numbers": true}],
        "quotes": [0, "single", "avoid-escape"],
        "radix": 1,
        "react/display-name": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-boolean-value": 1,
        "react/jsx-closing-bracket-location": 1,
        "react/jsx-curly-spacing": 1,
        "react/jsx-equals-spacing": 1,
        "react/jsx-handler-names": 1,
        "react/jsx-indent": 1,
        "react/jsx-indent-props": 1,
        "react/jsx-key": 1,
        "react/jsx-max-props-per-line": 0,
        "react/jsx-no-bind": 1,
        "react/jsx-no-duplicate-props": 1,
        "react/jsx-no-literals": 0,
        "react/jsx-no-undef": 0,
        "react/jsx-pascal-case": 1,
        "react/jsx-sort-props": 0,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/no-danger": 1,
        "react/no-deprecated": 0,
        "react/no-did-mount-set-state": 1,
        "react/no-did-update-set-state": 1,
        "react/no-direct-mutation-state": 1,
        "react/no-is-mounted": 1,
        "react/no-multi-comp": 0,
        "react/no-set-state": 0,
        "react/no-string-refs": 1,
        "react/no-unknown-property": 1,
        "react/prefer-es6-class": 0,
        "react/prop-types": 0,
        "react/react-in-jsx-scope": 0,
        "react/self-closing-comp": 1,
        "react/sort-comp": 0,
        "react/sort-prop-types": 1,
        "react/jsx-wrap-multilines": 1,
        "require-jsdoc": 0,
        "require-yield": 2,
        "semi": [1, "always"],
        "semi-spacing": [1, {"before": false, "after": true}],
        "sort-vars": 0,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": [1, {"int32Hint": false}],
        "space-unary-ops": [1, {"words": true, "nonwords": false}],
        "spaced-comment": [1, "always", {"exceptions": ["/"]}],
        "strict": [0, "global"],
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": [2, "inside"],
        "wrap-regex": 1,
        "yoda": [0, "never"],
    }
}
